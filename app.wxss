.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  font-size:12px; 
  color:#3e3e3e;
} 
.commonColor{
  color:#c96148;
}
.commonbgColor{
  background-color:#c96148;
}
.fr{
